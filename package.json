{
  "name": "lotus-ah-frontend",
  "version": "1.0.0",
  "description": "client implementation of the lotus author haven",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js src/**/*.jsx --fix",
    "start": "node app.js",
    "build": "babel-node ./node_modules/webpack/bin/webpack.js --config webpack.config.prod.js",
    "start:dev": "babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js --hot --color --open",
    "test": " NODE_ENV=test SERVER_URL=http://localhost:5000 jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "test:coverage": "jest --verbose --coverage"
  },
  "jest": {
    "transform": {
      ".js$": "babel-7-jest",
      ".jsx$": "babel-7-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/test-setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/lotus-ah-frontend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela/lotus-ah-frontend/issues"
  },
  "homepage": "https://github.com/andela/lotus-ah-frontend#readme",
